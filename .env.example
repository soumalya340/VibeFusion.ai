# Environment Variables for VibeFusion.ai

# API Keys
COINGECKO_API_KEY=your_coingecko_api_key
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_BEARER_TOKEN=your_twitter_bearer_token
NEWS_API_KEY=your_news_api_key
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key

# Blockchain
ALCHEMY_API_KEY=your_alchemy_api_key
INFURA_API_KEY=your_infura_api_key
PRIVATE_KEY=your_private_key_for_trading
WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id

# Database
REDIS_URL=redis://localhost:6379
MONGODB_URL=mongodb://localhost:27017/vibefusion

# Security
JWT_SECRET=your_jwt_secret_key_here
SESSION_SECRET=your_session_secret_here

# Server Configuration
BACKEND_PORT=5000
FRONTEND_URL=http://localhost:3000
BACKEND_URL=http://localhost:5000

# Trading Configuration
MAX_POSITION_SIZE_ETH=1.0
MIN_TRADE_SIZE_USD=10
SLIPPAGE_TOLERANCE=0.5
GAS_LIMIT_MULTIPLIER=1.2

# Agent Configuration
AGENT_WEBSOCKET_PORT=8080
DATA_SCRAPER_INTERVAL=30000
ANALYSIS_INTERVAL=60000
MONITORING_INTERVAL=15000

# External APIs
UNISWAP_V3_SUBGRAPH=https://api.thegraph.com/subgraphs/name/uniswap/uniswap-v3
ONEINCH_API_URL=https://api.1inch.io/v5.0/1

# Development
NODE_ENV=development
DEBUG=true
